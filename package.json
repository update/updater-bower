{
  "name": "updater-bower",
  "description": "Update bower.json with data from package.json.",
  "version": "1.0.2",
  "homepage": "https://github.com/update/updater-bower",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "update/updater-bower",
  "bugs": {
    "url": "https://github.com/update/updater-bower/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "updatefile.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "find-pkg": "^1.0.0",
    "is-valid-app": "^0.3.0",
    "sync-bower": "^1.0.1",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "copy": "^0.3.1",
    "delete": "^1.1.0",
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.3",
    "npm-install-global": "^1.0.0",
    "update": "^0.7.4"
  },
  "keywords": [
    "bower",
    "fix",
    "lint",
    "linter",
    "maintain",
    "maintainance",
    "plugin",
    "plugins",
    "up-to-date",
    "update",
    "update-plugin",
    "update-updater",
    "updateplugin",
    "updater",
    "upkeep"
  ],
  "verb": {
    "toc": false,
    "layout": null,
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "updater-contributing",
        "updater-contributors",
        "updater-eslint",
        "updater-package"
      ]
    },
    "reflinks": [
      "update",
      "updater-bower"
    ],
    "lint": {
      "reflinks": true
    }
  }
